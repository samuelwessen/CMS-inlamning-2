@using Umbraco.Cms.Web.Common.PublishedModels;
@inherits Umbraco.Cms.Web.Common.Views.UmbracoViewPage<ContentModels.NewsArticle>
@using ContentModels = Umbraco.Cms.Web.Common.PublishedModels;
@{
	Layout = "Master.cshtml";
}

<div>
    <div>
        <h1>@Model.Value("articleTeaserTitle")<h1>
    </div>
    <div>
        @{
		    var image = Model.Value<IPublishedContent>("articleTeaserImg");
		    }
		    <img src="@image.Url()"/>
    </div>
    <div>
        @Model.Value("newsArticleContent")
    </div>
</div>